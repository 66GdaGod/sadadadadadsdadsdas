game.Players.PlayerAdded:Connect(function(plr) 
	plr.CharacterAdded:Connect(function(Char)
		Char:WaitForChild("Humanoid")
		if plr.UserId == 92112259 then -- You may change to any player id.
			Char.Humanoid.DisplayName = "[â­]" .. plr.DisplayName -- Any emoji.
		end
	end)
end)

